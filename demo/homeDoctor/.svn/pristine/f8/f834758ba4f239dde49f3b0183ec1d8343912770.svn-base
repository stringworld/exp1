/* tab控制器 start*/
.tab-segmented-control{
    font-size: 0.7rem;
    font-weight: 400;
    position: relative;
    display: table;
    overflow: hidden;
    width: 100%;
    table-layout: fixed;
    background-color: #fff;
    -webkit-touch-callout: none;
}
.tab-control-item {
    line-height: 1.9rem;
    display: table-cell;
    overflow: hidden;
    width: 1%;
    -webkit-transition: background-color .1s linear;
    transition: background-color .1s linear;
    text-align: center;
    white-space: nowrap;
    text-overflow: ellipsis;
    color: #333;
}
.tab-control-item.tab-active {
    position: relative;
    background-color: #fff;
    color: #00B6F3;
}
.tab-control-item.tab-active:after {
    position: absolute;
    content: '';
    width: 5rem;
    bottom: 0;
    left: 50%;
    transform:translateX(-50%);
    -webkit-transform:translateX(-50%);
    border-bottom: 2px solid #00b6f3;
}
.tab-control-content{
    position: relative;
    display: none;
}

.tab-control-content.tab-active {
    display: block;
}
.item-media-object {
    width: 3rem;
    height: 3rem;
    border-radius: 50%;
    margin-right: 0.5rem;
}
.item-media,.item-media-body {
    overflow: hidden;
}
.item-ellipsis {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
h2.item-ellipsis{
    font-size:0.8rem;
}
h2.item-ellipsis span {
    font-size: 0.7rem;
    color: #4A4A4A;
    padding-left: 0.5rem;
}
p.item-ellipsis{
    color: #999;
    font-size:0.7rem;
    line-height: 0.85rem;
}
/* tab控制器 end*/

