    display: inline-block;
    padding: 0.5rem 0;
    margin: 0 0.3rem;
}

ul.tab li.active {
    color: #09BB07;
    border-bottom: 1px #09BB07 solid;
}

ul.tab-1 li {
    width: 72%;
}

ul.tab-2 li {
    width: 40%;
}

ul.tab-3 li {
    width: 24%;
}

ul.tab-radio {
    display: inline-block;
}

ul.tab-radio li {
    display: inline-block;
    margin-right: 0.55rem;
    padding: 0.2rem 0.6rem;
    border: 1px #ddd solid;
}

ul.tab-radio li.active {
    background-color: #09BB07;
    color: #fff;
}

.tab-piece {
    padding: 0.2rem 1rem;
    background-color: #fff;
}

.tab-piece .tab-piece-i {
    display: inline-block;
    width: 50%;
    text-align: center;
    border: 1px solid #09BB07;
    color: #09BB07;
    padding: 0.25rem;
}

.tab-piece .tab-piece-i:nth-child(1) {
    border-radius: 0.2rem 0 0 0.2rem;
}

.tab-piece .tab-piece-i:nth-child(2) {
    border-radius: 0 0.2rem 0.2rem 0;
}

.tab-piece .active {
    background-color: #09BB07;
    color: #fff;
}

/** tab标签 开始*/

/** search 开始*/
.key-search {
    background-color: #EFEFF4;
    padding: 0.3rem 0.5rem;
    position: relative;
    width: 100%;
}

.key-search input {
    border: none;
    border-radius: 0.15rem;
    padding: 0.1rem 1.2rem;
    outline: none;
    width: 100%;
}

.key-search .cancel {
    color: #09BB07;
    white-space: nowrap;
    position: absolute;
    top: 0.35rem;
    right: 0.5rem;
}

.key-search .wechat-icon-search {
    position: absolute;
    top: 30%;
    color: #B2B2B2;
}

.key-search .wechat-icon-clear {
    position: absolute;
    top: 30%;
    color: #B2B2B2;
    right: 2.5rem;
}

/** search 结束*/
