.form-group {
    background-color: #ffffff;
    border-top: #ddd solid 1px;
}

.form-group .form-item, .form-group .form-item-expand {
    position: relative;
    border-bottom: #ddd solid 1px;
    padding: 0.5rem 0.8rem;
}

.form-group .form-item-error {
    padding: 0.3rem 1.2rem;
    font-size: 0.6rem;
    color: red;
    background-color: #F7F6F6;
    border-bottom: #ddd solid 1px;
}

.form-group .form-item-expand .right {
    padding-right: 0.6rem;
}

.form-group .form-item-expand:before {
    content: " ";
    display: inline-block;
    height: 0.25rem;
    width: 0.25rem;
    border-width: 0.08rem 0.08rem 0 0;
    border-color: #D4D4D4;
    border-style: solid;
    transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
    position: absolute;
    top: 50%;
    right: 0.82rem;
    margin-top: -0.15rem;
    font-size: 0.65rem;
}

.form-group .form-control {
    background-color: transparent !important;
    border: none;
    outline: none;
    text-align: right;
    width: 9rem;
    position: absolute;
    right: 0.8rem;
    height: 100%;
    top: 0;
}

.form-group .form-control[type="date"] {
    opacity: 0;
}

.form-group select.form-control {
    direction: rtl;
    -webkit-appearance: none;
}

.form-group .touch-effect:active {
    background-color: #ededed;
}

.form-group .show-label {
    display: inline-block;
    width: 5rem;
}

