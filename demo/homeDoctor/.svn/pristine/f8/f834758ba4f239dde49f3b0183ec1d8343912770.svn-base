@charset "utf-8";

@import "reset.css";
@import "animate.css";
@import "../js/plugins/fullPage/jquery.fullPage.css";
@import "icon.css";

html,body{
    width: 100%;
    height: 100%;
    list-style: none;
    overflow-x: hidden;
    overflow-y: hidden;
}

body {
    font-size: 0.7rem;
    font-family: "微软雅黑", arial, serif;
    list-style: none;
    overflow-x: hidden;
    overflow-y: hidden;
    background-color: #f1f6f9;
}
h1,h2,h3,h4,h5{
    font-weight:normal;
}
input[type=search], input[type=text], input[type=password], input[type=datetime], input[type=datetime-local], input[type=date], input[type=month], input[type=time], input[type=week], input[type=number], input[type=email], input[type=url], input[type=tel], input[type=color], textarea {
    border: 0;
    display: block;
    padding-top: 0.1rem;
    padding-left: 0;
    height: 1.45rem;
    vertical-align: middle;
    font-size: 0.7rem;
    line-height: 0.8rem;
}
.main{
    height:100%!important;
    width:100%!important;
}
    /** 常用样式 开始 */

/*表单验证 start*/
.tip-show {
    margin-bottom: 1.2rem;
    position: relative;
}

.form-list q {
    display: none;
}

.tip-show q {
    display: block;
    position: absolute;
    z-index: 2;
    bottom: -1.005rem;
    right: 0.75rem;
    color: #f46161;
    font-size: 0.6rem;
}

.list {
    position: relative;
    padding-top: 1px;
    padding-bottom: 1px;
    padding-left: 0;
    margin-bottom: 0.8rem;
}

.marked_words {
    font-size: 0.7rem;
    font-weight: normal;
    margin: 0.3rem 0 0.6rem 0;
    height: 1rem;
    line-height: 1.3rem;
    padding: 0 0.75rem;
    color: #999;
}

.item {
    border-color: #ddd;
    background-color: #fff;
    color: #444;
    position: relative;
    z-index: 2;
    margin: -1px;
    border-width: 1px;
    border-style: solid;
    font-size: 0.7rem;
    padding: 0.8rem;
    display: block;
}

.item h2 {
    margin: 0 0 2px;
    font-size: 0.7rem;
    font-weight: 400;
}

.item p {
    color: #666;
    font-size: 0.65rem;
    margin-bottom: 2px;
}

.item-input {
    display: -webkit-box;
    display: -webkit-flex;
    display: -moz-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    -moz-align-items: center;
    align-items: center;
    position: relative;
    overflow: hidden;
    padding: 6px 0 5px 0.75rem;
}

.item-input input {
    -webkit-border-radius: 0;
    border-radius: 0;
    -webkit-box-flex: 1;
    -webkit-flex: 1 220px;
    -moz-box-flex: 1;
    -moz-flex: 1 220px;
    -ms-flex: 1 220px;
    flex: 1 220px;
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    margin: 0;
    background-color: transparent;
    color: #333;
    outline: none;
}

.list .item-input input {
    position: absolute;
    right: 0;
    text-align: right;
    padding-right: 0.75rem;
    max-width: 65%;
}

.item-icon-left .icon, .item-icon-right .icon {
    display: -webkit-box;
    display: -webkit-flex;
    display: -moz-box;
    display: -moz-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    -moz-align-items: center;
    align-items: center;
    position: absolute;
    top: 0;
    height: 100%;
    font-size: 1.45rem;
    color: #999;
}

.item-icon-left {
    padding-left: 2.7rem;
}

.item-icon-right {
    padding-right: 2.7rem;
}

.item-icon-right .icon {
    right: 0.75rem;
}

.item-icon-left .icon {
    left: 0.75rem;
}

.input-label {
    display: table;
    padding: 0.35rem 0.25rem 0.35rem 0;
    max-width: 200px;
    color: #444;
    font-size: 0.7rem;
}

.selectStores span {
    display: block;
    max-width: 72%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #333;
    font-size: 0.7rem;
    float: right;
}

/*提交按钮 start*/
.button {
    border-color: transparent;
    background-color: #f8f8f8;
    color: #444;
    position: relative;
    display: inline-block;
    margin: 0;
    padding: 0 0.6rem;
    min-width: 52px;
    min-height: 47px;
    border-width: 1px;
    border-style: solid;
    border-radius: 4px;
    vertical-align: top;
    text-align: center;
    text-overflow: ellipsis;
    font-size: 0.8rem;
    line-height: 2.1rem;
    cursor: pointer;
}

.button.disabled, .button[disabled] {
    opacity: .4;
    cursor: default !important;
    pointer-events: none;
}

.button-block {
    display: block;
    clear: both;
}

.button-sub {
    margin: 20px auto;
    letter-spacing: 5px;
    width: 85% !important;
    border-color: transparent;
    color: #fff;
}

/*提交按钮 end*/
.item-button {
    position: absolute;
    right: 0.4rem;
    min-height: 33px;
    height: 1.6rem;
    line-height: 1.6rem;
}

.yzm.item input {
    position: relative;
    max-width: 40%;
    text-align: center;
}

.yzm.item .item-button {
    font-size: 0.6rem;
    line-height: 0.7rem;
    background-color: #00b6f3;
    color: #fff;
}

.spacing2 {
    letter-spacing: 8px;
}

/*表单验证 end*/
.row {
    padding: 5px;
    width: 100%;
    overflow: hidden;
    text-align: center;
}

/* 抬头标示语 start*/
.text-jumbotron {
    display: table;
    width: 100%;
    height: 4rem;
    color: #777;
}

.text-jumbotron div {
    display: table-cell;
    vertical-align: middle;
    text-align: center;
}

/* 抬头标示语 end*/
/* 必填项 */
.redstars {
    font-style: normal;
    font-weight: normal;
    color: red;
}

/* 文本域 */
textarea {
    width: 100%;
    height: 5rem;
    border: none;
    color: #9B9B9B;
    font-size: 0.65rem;
    resize: none;
    outline: none;
    padding: 10px 15px 5px;
    line-height: 1.05rem;
}

.TextNum {
    width: 100%;
    background: #ffffff;
    color: #9B9B9B;
    font-size: 0.65rem;
    text-align: right;
    padding: 0 15px 10px;
}

/* 上传图片 start*/
.upload {
    position: relative;
    border-top: 1px solid #CCCCCC;
    padding: 10px 15px 15px;
    overflow: hidden;
    background: #ffffff;
}

.upload p {
    font-size: 0.7rem;
    margin-bottom: 0.7rem;
    color: #8f8f94;
}

.upload .button-select {
    float: left;
    display: block;
    background: rgb(174, 174, 174);
    width: 2.5rem;
    height: 2.5rem;
    position: relative;
}

.upload .button-select i {
    display: block;
    width: 1.5rem;
    height: 1.5rem;
    margin: 0.5rem auto;
    background: url(../img/btn_yongyao_add.png) no-repeat scroll top left;
    background-size: cover;
}

.upload input[type=file] {
    position: absolute;
    width: 50px;
    height: 50px;
    left: 0;
    top: 0;
    opacity: 0;
    filter: alpha(opacity=0);
}

.upload .img-div {
    width: 2.5rem;
    height: 2.5rem;
    position: relative;
    float: left;
    margin: 0 0 0 0.5rem;
}

.upload img {
    width: 100%;
    height: 100%;
}

.upload .img-del {
    font-size: 1rem;
    text-align: center;
    position: absolute;
    display: block;
    width: 1.5rem;
    height: 1.5rem;
    line-height: 1.5rem;
    color: red;
    top: -0.75rem;
    right: -0.75rem;
}

/* 上传图片 end*/

/** iscroll 开始*/

.wrapper {
    position: absolute;
    z-index: 1;
    top: 0;
    bottom: 0;
    left: 0;
    width: 100%;
    overflow: hidden;
}

.scroller {
    position: absolute;
    z-index: 1;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    width: 100%;
    -webkit-transform: translateZ(0);
    -moz-transform: translateZ(0);
    -ms-transform: translateZ(0);
    -o-transform: translateZ(0);
    transform: translateZ(0);
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    -webkit-text-size-adjust: none;
    -moz-text-size-adjust: none;
    -ms-text-size-adjust: none;
    -o-text-size-adjust: none;
    text-size-adjust: none;
}
